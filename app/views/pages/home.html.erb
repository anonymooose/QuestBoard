<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>

    <%= form_tag(events_path, :method => "get", id: "search-form", class: "center-block") do %>
      <div class="form-group">
        <%= text_field_tag :search_loc, params[:search_loc], placeholder: "location", class: "form-control input-size-large" %>
        <%= date_select :search_date, params[:search_date], placeholder: "date", class: "form-control input-size-large" %>
        <%= select_tag :search_game, params[:search_game], placeholder: "game", class: "form-control input-size-large" %>
      </div>
      <%= submit_tag "search", :name => nil, class: "center-block btn btn-primary font-weight-extra-bold" %>
      <% end %>
      </div>

